{
  "name": "@mlz/admin",
  "version": "0.5.74",
  "description": "一套编程猫设计规范下的管理系统React组件库，基于Antd",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "codemao",
    "antd"
  ],
  "scripts": {
    "start": "npm run clean && node ./scripts/genIndex && NODE_ENV=development dumi dev",
    "build": "node ./scripts/genIndex && NODE_ENV=production dumi build",
    "dist": "npm run clean && node ./scripts/genIndex && father-build",
    "dist:es": "npx ttsc --project ./tsconfig.dist.json -d -m esnext --outDir es --sourceMap false --inlineSources false",
    "dist:cjs": "npx ttsc --project ./tsconfig.dist.json -d -m commonjs --outDir lib --sourceMap false --inlineSources false",
    "dist:tsc": "rm -rf es lib dist && npm run dist:cjs && npm run dist:es",
    "serve": "node ./scripts/serve",
    "lint": "npm run lint:ts && npm run lint:es",
    "lint:fix": "npm run lint:ts && npm run lint:es --fix",
    "lint:esFix": "npx eslint --ext tsx,ts,js src --fix",
    "lint:es": "npx eslint --ext tsx,ts,js src docs/demo",
    "lint:ts": "tsc --project ./tsconfig.lint.json",
    "lint:style": "stylelint 'src/**/*.less' --syntax less",
    "test": "npm run clean && node ./scripts/genIndex && dumi test --coverage",
    "test:update": "npm run clean && node ./scripts/genIndex && dumi test --cache=false -u --coverage",
    "check-coverage": "npx istanbul check-coverage --statement 90 --branch -80",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "version": "AUTO=1 node ./scripts/versioning",
    "clean": "rm -rf es lib dist .tsc-check coverage src/index.tsx",
    "changelog": "node ./scripts/changelog",
    "reset-project": "npm run clean && rm -rf node_modules package-lock.json yarn.lock src/.umi && npm install"
  },
  "module": "es/index.js",
  "main": "lib/index.js",
  "unpkg": "unpkg/mlz-admin.min.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "unpkg",
    "LICENSE"
  ],
  "gitHooks": {
    "pre-commit": "npx lint-staged && npx pretty-quick --staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@mlz/axios": "^1.1.3",
    "antd": "^4.7.0",
    "axios": "^0.21.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "lodash-es": "^4.17.15",
    "mytils": "^1.3.21",
    "react": "^16.12.0"
  },
  "peerDependencies": {
    "react": ">=16.12.0",
    "antd": ">=4.6.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.13",
    "@types/lodash-es": "^4.17.3",
    "@umijs/preset-react": "^1.6.4",
    "@umijs/test": "^3.2.19",
    "bundlesize": "^0.18.0",
    "dumi": "^1.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.0.2",
    "father-build": "^1.18.4",
    "inquirer": "^7.3.3",
    "istanbul": "^0.4.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.3.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "simple-git": "^2.21.0",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "ttypescript": "^1.5.10",
    "typescript-transform-paths": "^2.0.1",
    "yorkie": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/juicecube/mlz-admin/issues"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/juicecube/mlz-admin"
  },
  "homepage": "https://mlz-admin.com",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
